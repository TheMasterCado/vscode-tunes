{
	"name": "vscode-tunes",
	"displayName": "VSCode tunes",
	"description": "Music community for programmers",
	"version": "1.1.0",
	"publisher": "TheMasterCado",
	"license": "MIT",
	"icon": "media/vscode_tunes_logo.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/TheMasterCado/vscode-tunes"
	},
	"bugs": {
		"url": "https://github.com/TheMasterCado/vscode-tunes/issues"
	},
	"engines": {
		"vscode": "^1.53.0"
	},
	"categories": [
		"Other"
	],
	"keywords": [
		"spotify",
		"music",
		"social",
		"friends"
	],
	"activationEvents": [
		"*"
	],
	"main": "./dist/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "vscode-tunes-sidebar-view",
					"title": "VSCode tunes",
					"icon": "media/tunes.png"
				}
			]
		},
		"views": {
			"vscode-tunes-sidebar-view": [
				{
					"type": "webview",
					"id": "vscode-tunes-sidebar",
					"name": "VSCode tunes",
					"icon": "media/tunes.png",
					"contextualTitle": "VSCode tunes"
				}
			]
		},
		"commands": [
			{
				"command": "vscode-tunes.authenticate",
				"category": "VSCode tunes",
				"title": "Authenticate"
			},
			{
				"command": "vscode-tunes.reset",
				"category": "VSCode tunes",
				"title": "Reset"
			},
			{
				"command": "vscode-tunes.toggle-playback",
				"category": "VSCode tunes",
				"title": "Toggle playback"
			}
		],
		"keybindings": [
			{
				"command": "vscode-tunes.toggle-playback",
				"key": "alt+`",
				"mac": ""
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"publish": "vsce publish",
		"compile": "webpack",
		"watch": "concurrently \"rollup -c -w\" \"webpack --watch\"",
		"package": "rollup -c && webpack --mode production --devtool hidden-source-map",
		"test-compile": "tsc -p ./",
		"test-watch": "tsc -watch -p ./",
		"pretest": "npm run test-compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@rollup/plugin-commonjs": "^14.0.0",
		"@rollup/plugin-node-resolve": "^8.0.0",
		"@rollup/plugin-typescript": "^6.0.0",
		"@tsconfig/svelte": "^1.0.0",
		"@types/glob": "^7.1.3",
		"@types/lodash-es": "^4.17.4",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/polka": "^0.5.2",
		"@types/vscode": "^1.53.0",
		"@typescript-eslint/eslint-plugin": "^4.14.1",
		"@typescript-eslint/parser": "^4.14.1",
		"concurrently": "^5.3.0",
		"css-loader": "^5.1.0",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"rollup": "^2.39.0",
		"rollup-plugin-svelte": "^6.1.1",
		"rollup-plugin-terser": "^7.0.2",
		"style-loader": "^2.0.0",
		"svelte": "^3.0.0",
		"svelte-check": "^1.0.0",
		"svelte-preprocess": "^4.0.0",
		"ts-loader": "^8.0.14",
		"typescript": "^4.1.3",
		"vsce": "^1.85.1",
		"vscode-test": "^1.5.0",
		"webpack": "^5.19.0",
		"webpack-cli": "^4.4.0"
	},
	"dependencies": {
		"@types/spotify-web-api-node": "^5.0.0",
		"axios": "^0.21.1",
		"lodash-es": "^4.17.20",
		"polka": "^0.5.2",
		"spotify-web-api-node": "^5.0.2",
		"vscode-codicons": "0.0.14"
	}
}
